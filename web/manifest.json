{
    "manifest_version": 3,
    "name": "Open litner",
    "version": "1.0",
    "description": "A Flutter web app running as a Chrome extension",
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/Icon-192.png"
    }
  }